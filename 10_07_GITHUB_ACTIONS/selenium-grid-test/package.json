{
  "name": "selenium-grid-test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx mocha tests/**/*.js --timeout 40000",
    "test:chrome": "npx mocha tests/**/*.js --grep \"chrome\" --timeout 40000",
    "test:firefox": "npx mocha tests/**/*.js --grep \"firefox\" --timeout 40000",
    "test:parallel": "npx mocha tests/**/*.js --parallel --timeout 40000",
    "start:grid": "docker-compose up -d",
    "stop:grid": "docker-compose down",
    "test:with-grid": "node run-tests.js"
  },
  "devDependencies": {
    "chai": "^5.1.1",
    "mocha": "^10.2.0",
    "selenium-webdriver": "^4.22.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Selenium Grid con Docker para Chrome y Firefox"
}